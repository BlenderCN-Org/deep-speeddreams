      <attstr name="module" val="LaneDriver"/>
      <attnum name="extended" val="0"/>
      <attnum name="skin targets" val="1"/>
    </section>

    <section name="5">
      <attnum name="idx" val="17"/>
      <attstr name="module" val="LaneDriver"/>
      <attnum name="extended" val="0"/>
      <attnum name="skin targets" val="1"/>
    </section>

    <section name="6">
      <attnum name="idx" val="18"/>
      <attstr name="module" val="LaneDriver"/>
      <attnum name="extended" val="0"/>
