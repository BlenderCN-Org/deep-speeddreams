From: Andre Netzeband <andre.netzeband@hm.edu>
Date: Sun, 14 May 2017 23:42:16 +0200
Subject: [PATCH] Reduce the resolution.

---
 data/config/screen.xml | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/data/config/screen.xml b/data/config/screen.xml
index 771b88f..18b0e0f 100644
--- a/data/config/screen.xml
+++ b/data/config/screen.xml
@@ -4,8 +4,8 @@
 
 <params name="Screen" version="1.19">
   <section name="Validated Screen Properties">
-    <attnum name="window width" val="1920"/>
-    <attnum name="window height" val="1200"/>
+    <attnum name="window width" val="640"/>
+    <attnum name="window height" val="480"/>
     <attnum name="bpp" val="32"/>
     <attstr name="full-screen" in="yes,no" val="no"/>
     <attstr name="stereo-vision" in="yes,no" val="no"/>
